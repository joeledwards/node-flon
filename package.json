{
  "name": "flon",
  "version": "1.5.0",
  "description": "FLat Object Notation compatible with gron (grep-able JSON)",
  "repository": "github:joeledwards/node-flon",
  "main": "index.js",
  "files": [
    "package.json",
    "README.md",
    "bin/*",
    "lib/**/*.js"
  ],
  "bin": {
    "flon": "bin/flon.js"
  },
  "scripts": {
    "build": "npm ci && npm run lint && npm test",
    "lint": "standard",
    "lint:fix": "standard --fix",
    "test": "tap ./test/*.js"
  },
  "keywords": [
    "flon",
    "json",
    "flat",
    "object",
    "notation",
    "gron"
  ],
  "author": "Joel Edwards <joeledwards@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@buzuli/color": "^2.2.2",
    "axios": "^0.21.1",
    "buffered-stream": "0.0.1",
    "clarinet": "^0.12.4",
    "durations": "^3.4.2",
    "ramda": "^0.27.1",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "standard": "^14.3.4",
    "stream-buffers": "^3.0.2",
    "tap": "^14.10.8"
  }
}
